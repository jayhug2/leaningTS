{
  "compilerOptions": {
    "module": "commonjs",                                
    "moduleResolution": "node",
    "target": "es2016",                                  
    "baseUrl": ".",
    "outDir": "dist",
    "paths": { "*": ["node_modules/*"]},
    "esModuleInterop": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "noImplicitAny": false,                    
  },
  "include": ["src/**/*"]
}
